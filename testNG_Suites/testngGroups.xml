<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite2">
	<groups>
		<run>
			<include name="sanity"></include>
		</run>
	</groups>

	<listeners>
		<listener class-name="org.Appium.Utils.Listeners"></listener>
	</listeners>

	<test thread-count="5" name="FillForm positive and negative">
		<classes>
			<class name="GeneralStore.eCommerce_TC1_fillForm" />
		</classes>
	</test>
	<test thread-count="5" name="Added to cart">
		<classes>
			<class name="GeneralStore.eCommerce_TC3_AddToCart" />
		</classes>
	</test>

</suite>
